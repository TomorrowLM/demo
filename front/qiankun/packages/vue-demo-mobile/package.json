{
  "name": "vue2-mobile",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12",
    "pnpm": ">=7"
  },
  "scripts": {
    "nvm": "nvm use v18.17.0",
    "serve": "npm run nvm && cross-env APP_ENV=local.dev vue-cli-service serve",
    "serve:qiankun": "npm run nvm && cross-env APP_ENV=local.qiankun vue-cli-service serve",
    "build": "npm run nvm && cross-env APP_ENV=production && vue-cli-service build --mode production",
    "build:qiankun": "npm run nvm && cross-env APP_ENV=production.qiankun vue-cli-service build --mode production",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "callapp-lib": "^3.5.1",
    "core-js": "^3.8.3",
    "echarts": "^5.3.3",
    "element-ui": "^2.15.6",
    "moment": "^2.29.1",
    "register-service-worker": "^1.7.1",
    "vant": "^2.12.35",
    "vue": "^2.6.14",
    "vue-ls": "^4.0.0",
    "vue-router": "^3.5.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@lm/cli": "workspace:^",
    "@lm/lint": "workspace:^",
    "@lm/shared": "workspace:^",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-service": "~5.0.9",
    "autoprefixer": "^9.8.4",
    "babel-plugin-lodash": "3.3.4",
    "cross-env": "^7.0.3",
    "node-sass": "^9.0.0",
    "postcss-pxtorem": "5.1.1",
    "sass": "^1.77.6",
